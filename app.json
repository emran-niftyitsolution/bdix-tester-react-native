{
  "expo": {
    "name": "bdix-tester-android",
    "slug": "bdix-tester-android",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#686E7C"
    },
    "ios": {
      "icon": "./assets/images/icon.jpg",
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/images/icon.jpg",
      "package": "com.ibnshayed.bdixtesterandroid"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.jpg"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dccd7366-624f-408b-a6df-fc7638bf60d2"
      }
    }
  }
}
